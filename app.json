{
  "expo": {
    "name": "Perfect Fit",
    "slug": "perfect-fit",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/loading.gif",
      "resizeMode": "contain",
      "backgroundColor": "#fff7eb"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app accesses the library to upload pictures to create puzzel."
      },
      "bundleIdentifier": "perfect-fit"
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#fff7eb"
      },
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.hemanth_reddy_tekula.perfectfit"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "998aac59-e020-4832-bb69-3b337056d043"
      }
    },
    "owner": "hemanth_reddy_tekula"
  }
}